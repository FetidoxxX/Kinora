<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Estado: deshabilitado (silla ocupada) -->
    <item android:state_enabled="false" android:color="#13121E"/>
    <!-- Estado: presionado o seleccionado por el usuario -->
    <item android:state_checked="true" android:color="#6C6C98"/>
    <!-- Estado: por defecto (disponible) -->
    <item android:color="#EADDFF"/>
</selector>