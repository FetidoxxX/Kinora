<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nav_bar">


    <ImageView
        android:id="@+id/Fondo"
        android:layout_width="351dp"
        android:layout_height="75dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="652dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nav_bar_fondo" />

    <ImageButton
        android:id="@+id/btnCines"
        android:layout_width="37dp"
        android:layout_height="49dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/Fondo"
        app:layout_constraintEnd_toEndOf="@+id/Fondo"
        app:layout_constraintStart_toStartOf="@+id/Fondo"
        app:layout_constraintTop_toTopOf="@+id/Fondo"
        app:srcCompat="@drawable/cines" />

    <ImageButton
        android:id="@+id/btnUsuario"
        android:layout_width="37dp"
        android:layout_height="49dp"
        android:layout_marginStart="75dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/Fondo"
        app:layout_constraintEnd_toEndOf="@+id/Fondo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnCines"
        app:layout_constraintTop_toTopOf="@+id/Fondo"
        app:layout_constraintVertical_bias="0.514"
        app:srcCompat="@drawable/usuario" />

    <ImageButton
        android:id="@+id/btnPeliculas"
        android:layout_width="37dp"
        android:layout_height="49dp"
        android:layout_marginEnd="75dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="@+id/Fondo"
        app:layout_constraintEnd_toStartOf="@+id/btnCines"
        app:layout_constraintHorizontal_bias="0.982"
        app:layout_constraintStart_toStartOf="@+id/Fondo"
        app:layout_constraintTop_toTopOf="@+id/Fondo"
        app:layout_constraintVertical_bias="0.514"
        app:srcCompat="@drawable/peliculas" />
</androidx.constraintlayout.widget.ConstraintLayout>